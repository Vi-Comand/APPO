<!DOCTYPE html>
<html lang="ru" class="bg-11 f-size16">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>@ViewData["Title"] - АППО (Автоматизированая платформа персонифицированного обучения Краснодарского края)</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/jquery.maskedinput.min.js"></script>


    <link href="~/lib/bootstrap-icons/font/bootstrap-icons.css" rel="stylesheet" />
</head>
<body class="bg-trans">
    <canvas width="1080" height="1980" id="stars"> </canvas>
    @*<div id="bg-31" style="z-index: -1; width:100%; height:100%; position:fixed; top:0px"></div>*@
    <nav class="navbar navbar-expand-lg navbar-light navbar1 text-white bg-nav align-items-start"></nav>
    <div class="container d-flex align-items-center">
        <main role="main" class="pb-3 bd-highlight ms-auto me-auto">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted " style="position:absolute">
        <div class="container " onclick="qwe()">
            <h5 class="a"> &copy; 2021 - АППО</h5>
        </div>
    </footer>
    
    
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/three.min.js"></script>
    @*<script src="~/js/bg-31.js"></script>*@
    <script src="~/js/stars.js"></script>
    <script src="~/js/sota.js"></script>
    
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="~/js/jquery.maskedinput.min.js"></script>
    <script>
            var anim = 0;
            function qwe() {
        @*<script src="~/js/bg-31.js"></script>*@
                if (anim == 0) {
                    init();
                    animate();
                    anim = 1;
                }
            };
    </script>

    <script>

    </script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
